<h2 align="center">Categories</h2>
<div class="container-fluid" align="center">
	<% @categories.each do |t| %>
		<ul class="listing">
			<div class="row">
				<div class="well col-md-4 col-md-offset-4">
					<li class="article-title">
						<%= link_to("#{t.name}", category_path(t)) %>
					</li>
				</div>
			</div>
		</ul>
	<% end %>
</div>
