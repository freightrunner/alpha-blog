<h1 align="center">AlphaBlog Users</h1>
<div class="container-fluid", align="center">
	<%= will_paginate %>
	<% @users.each do |t| %>
		<ul class="listing">
			<div class="row">
				<div class="well col-md-4 col-md-offset-4">
					<li><%= link_to(gravatar_for(t), user_path(t)) %></li>
					<li class="article-title"><%= link_to(t.username, user_path(t)) %></li>
					<li><small><%= pluralize(t.articles.count, "article") if t.articles %></small></li>
				</div>
			</div>
		</ul>
	<% end %>
	<%= will_paginate %>
</div>
